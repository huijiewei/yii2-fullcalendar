{
  "name": "huijiewei/yii2-fullcalendar",
  "description": "A full-sized drag & drop event calendar (jQuery plugin).",
  "minimum-stability": "stable",
  "license": "MIT",
  "type": "yii2-extension",
  "homepage": "https://github.com/huijiewei/yii2-fullcalendar.git",
  "keywords": [
    "yii2",
    "extension",
    "fullcalendar",
    "assets"
  ],
  "authors": [
    {
      "name": "Huijie Wei",
      "email": "huijiewei@gmail.com"
    }
  ],
  "require": {
    "php": "^7.1.3",
    "yiisoft/yii2": "^2.0",
    "huijiewei/yii2-moment": "^2.0",
    "npm-asset/fullcalendar": "^3.10"
  },
  "autoload": {
    "psr-4": {
      "huijiewei\\fullcalendar\\": "src"
    }
  },
  "repositories": {
    "asset": {
      "type": "composer",
